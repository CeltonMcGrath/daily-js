    "babel-plugin-transform-es2015-modules-commonjs": "^6.26.2",
    "rollup": "^1.10.1",
    "rollup-plugin-cjs-es": "^0.7.0",
    "rollup-plugin-commonjs": "^9.3.4",
    "rollup-plugin-node-resolve": "^4.2.3",
    "webpack": "^4.29.6",
    "webpack-cli": "^3.3.0"
  },
  "dependencies": {
    "@babel/runtime": "^7.4.3",
    "events": "^3.0.0",
    "fast-equals": "^1.6.2",
